{
  "name": "levi-math",
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p serve start:* seed",
    "serve": "nx run-many --parallel --target=serve --all --max-parallel=100",
    "build": "nx run-many --parallel --target=build --all --max-parallel=100 --configuration=production",
    "test": "nx run-many --parallel --target=test --all --max-parallel=100",
    "start:db": "rimraf ./data/db && rimraf ./data/mongod.log && mkdirp ./data/db && mongod --dbpath ./data/db --logpath ./data/mongod.log",
    "start:watchers": "chokidar \"libs/gql/**/*.gql\" -c \"npm run generate:gql\"",
    "generate:gql": "nx run gql:generate",
    "seed": "mongorestore -d=timesheet ./tools/seed/dump/timesheet",
    "seed:update": "npx ts-node ./tools/seed/setup-seed.ts",
    "prepare": "husky install",
    "lint": "nx run-many --parallel --all --max-parallel=100 --target=lint --fix",
    "typecheck": "nx run-many --parallel --all --max-parallel=100 --target=typecheck"
  },
  "private": true,
  "lint-staged": {
    "*": "npx nx format:write --verbose --files "
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "14.5.4",
    "@nrwl/cypress": "14.5.4",
    "@nrwl/eslint-plugin-nx": "14.5.4",
    "@nrwl/jest": "14.5.4",
    "@nrwl/linter": "14.5.4",
    "@nrwl/nest": "^14.5.4",
    "@nrwl/node": "14.5.4",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "^14.5.4",
    "@nrwl/web": "14.5.4",
    "@nrwl/workspace": "14.5.4",
    "@testing-library/react": "13.3.0",
    "@types/jest": "27.4.1",
    "@types/math-expression-evaluator": "^1.3.0",
    "@types/node": "16.11.7",
    "@types/prompts": "^2.0.14",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
